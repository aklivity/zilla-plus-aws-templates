{
  "name": "secure-public-access",
  "version": "1.0.0",
  "main": "main.js",
  "types": "main.ts",
  "license": "MPL-2.0",
  "private": true,
  "scripts": {
    "get": "cdktf get",
    "build": "tsc",
    "synth": "NODE_OPTIONS='--max-old-space-size=8192' cdktf synth",
