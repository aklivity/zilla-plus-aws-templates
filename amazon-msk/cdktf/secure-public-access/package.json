    "aws-cdk-lib": "^2.158.0",
    "cdktf": "^0.20.8",
    "constructs": "^10.3.0",
    "mustache": "^4.2.0",
    "netmask": "^2.0.2"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/mustache": "^4.2.5",
    "@types/netmask": "^2.0.5",
    "@types/node": "^20.14.10",
    "cdktf-cli": "^0.20.8",
    "jest": "^29.7.0",
    "ts-jest": "^29.2.2",
